# Prisma database connection for Supabase Postgres or any Postgres instance
DATABASE_URL="postgresql://postgres:postgres@localhost:54322/postgres"
# DIRECT_URL is optional; keep empty unless you use a pooled connection string
DIRECT_URL=""

# Supabase project public keys used by the frontend and auth flows
NEXT_PUBLIC_SUPABASE_URL="https://your-project-id.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="public-anon-key"

# Service role key is only needed for admin-level actions (optional)
SUPABASE_SERVICE_ROLE_KEY=""

# Set development/runtime environment flags if needed
NEXT_PUBLIC_ENV="development"
